<?xml version="1.0" encoding="utf-8"?>
<com.debut.ellipsis.freehit.About.ElasticDragDismissFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/draggable_frame"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:dragDismissDistance="@dimen/_100sdp"
    app:dragDismissScale="0.95">

    <android.support.v4.view.ViewPager
        android:id="@+id/pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.debut.ellipsis.freehit.About.InkPageIndicator
        android:id="@+id/indicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|center_horizontal"
        android:layout_marginTop="@dimen/_15sdp"
        app:pageIndicatorColor="@color/colorSecondaryLight"
        app:currentPageIndicatorColor="@color/colorSecondary" />

</com.debut.ellipsis.freehit.About.ElasticDragDismissFrameLayout>